<scroll-view class="DrawerPage" style="{{('background-color: '+(skin?'#ffffff':'#ffffff')+';')}}" scroll-y="{{true}}" scroll-with-animation="true"><image class="cu-avatar bg-blue lg" style="width:100%;height:500rpx;" src="https://cdn.jzzz66.cn/ranking_bg.png" mode="widthFix"></image><scroll-view class="bg-blue nav" scroll-x="{{true}}"><view class="flex text-center"><block wx:for="{{2}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item','flex-sub',index==TabCur?'text-white':'text-gray']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+(index===1?'升序':"降序")+''}}</view></block></view></scroll-view><block wx:for="{{timeArticle}}" wx:for-item="value" wx:for-index="key" wx:key="*this"><block><view class="cu-timeline"><view class="cu-time">{{key}}</view><block wx:for="{{value}}" wx:for-item="item" wx:for-index="index1" wx:key="index1"><block><view class="cu-item text-green"><view class="{{['content','shadow-blur','bg-'+ColorList[index1]]}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['gotoArticle',['$event']]]]]}}" bindtap="__e">{{''+item.title+''}}</view></view></block></block></view></block></block></scroll-view>